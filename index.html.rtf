\par \hich\af37\dbch\af31505\loch\f37 \hich\f37         /** Divide el texto del textarea en un array de p\'e1\loch\f37 rrafos. */
\par \hich\af37\dbch\af31505\loch\f37         function parseParagraphs() \{
\par \hich\af37\dbch\af31505\loch\f37             const rawText = textToReadEl.value;
