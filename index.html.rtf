\par \hich\af37\dbch\af31505\loch\f37 \hich\f37         /** Resalta el p\'e1\loch\f37 rrafo actual en el textarea. */
\par \hich\af37\dbch\af31505\loch\f37         function scrollToCurrentParagraph(startChar = null, endChar = null) \{
\par \hich\af37\dbch\af31505\loch\f37             if (currentParagraphIndex < \hich\af37\dbch\af31505\loch\f37 0 || currentParagraphIndex >= paragraphs.length) return;
\par \hich\af37\dbch\af31505\loch\f37             
\par \hich\af37\dbch\af31505\loch\f37             const text = textToReadEl.value;
\par \hich\af37\dbch\af31505\loch\f37             let charIndex = 0;
\par \hich\af37\dbch\af31505\loch\f37             const newlineRegex = /\\r?\\n\\s*\\r?\\n/; 
\par 
\par \hich\af37\dbch\af31505\loch\f37             const rawParagraphs = text.split(newlineRegex);
\par \hich\af37\dbch\af31505\loch\f37             let pStart = 0;
