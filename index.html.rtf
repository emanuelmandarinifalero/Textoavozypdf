\par \hich\af37\dbch\af31505\loch\f37       \hich\af37\dbch\af31505\loch\f37 \hich\f37       // 3. Preparaci\'f3\loch\f37 n de la voz (nueva utterance)
\par \hich\af37\dbch\af31505\loch\f37             utterance = new SpeechSynthesisUtterance(cleanedText);
