\par 
\par \hich\af37\dbch\af31505\loch\f37 \hich\f37         /** Carga el texto de ejemplo en el textarea seg\'fa\loch\f37 n el idioma. */
\par \hich\af37\dbch\af31505\loch\f37         function updateTextByLanguage(lang) \{
